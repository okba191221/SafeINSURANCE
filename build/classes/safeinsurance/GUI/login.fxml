            <PasswordField fx:id="txtpassword" alignment="CENTER" layoutX="14.0" layoutY="10.0" promptText="mot de passe" style="-fx-background-color: transparent; -fx-border-color: #87CEFA; -fx-border-width: 0px 0px 3px 0px;">
               <font>
                  <Font name="Arial Bold" size="14.0" />
